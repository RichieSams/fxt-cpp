{
    "testMate.cpp.test.advancedExecutables": [
        {
            "pattern": "build/*/tests/src/fxt-test*",
            "cwd": "${absDirpath}",
            "runTask": {
                "before": [
                    "build"
                ]
            }
        }
    ],
    "testMate.cpp.debug.configTemplate": {
        "type": "cppvsdbg",
        "linux": {
            "type": "cppdbg",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        "darwin": {
            "type": "cppdbg",
            "MIMode": "lldb"
        },
        "win32": {
            "type": "cppvsdbg"
        },
        "program": "${exec}",
        "args": "${argsArrayFlat}",
        "cwd": "${cwd}",
        "env": "${envObj}",
        "environment": "${envObjArray}",
        "sourceFileMap": "${sourceFileMapObj}",
        "stopAtEntry": false,
        "externalConsole": false
    },
    "testMate.cpp.debug.breakOnFailure": true,
    "files.associations": {
        "bitset": "cpp",
        "string": "cpp",
        "vector": "cpp",
        "memory": "cpp",
        "optional": "cpp",
        "string_view": "cpp",
        "system_error": "cpp",
        "variant": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "cctype": "cpp",
        "chrono": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "compare": "cpp",
        "concepts": "cpp",
        "condition_variable": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdint": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "deque": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
        "exception": "cpp",
        "algorithm": "cpp",
        "functional": "cpp",
        "iterator": "cpp",
        "memory_resource": "cpp",
        "numeric": "cpp",
